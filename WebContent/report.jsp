<center> <h1>Feedback Report</h1> </center>

<hr>
  
  <form class="form-horizontal" action="reportgen" method="post">
    <div class="form-group">
      <label class="control-label col-sm-3" for="Subjectcode">Subject Code:</label>
      <div class="col-sm-5">
        <input type="text" class="form-control" id="scode" placeholder="Subject Code" name="scode">
      </div>
    </div>
    <div class="form-group">
      <label class="control-label col-sm-3" for="teachercode">Teacher's Code:</label>
      <div class="col-sm-5">          
        <input type="text" class="form-control" id="tcode" placeholder="Teacher's Code" name="tcode">
      </div>
    </div>
    
    
    <div class="form-group">        
      <div class="col-sm-offset-2 col-sm-10">
        <button type="submit" class="btn btn-primary">Submit</button>
      </div>
    </div>
  </form>

